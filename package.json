{
  "$schema": "https://json.schemastore.org/package",
  "name": "mktable",
  "files": [
    "dist"
  ],
  "version": "1.0.0",
  "description": "CLI tool that outputs a table of contents based on a markdown file headers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbragagt/type-envd.git"
  },
  "bin": {
    "mktable": "dist/index.js"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts",
    "lint": "oxlint src",
    "typecheck": "tsc --noEmit",
    "prepare": "[ -d '.husky' ] && husky install || true"
  },
  "keywords": [
    "markdown",
    "table",
    "contents",
    "cli"
  ],
  "author": "wesbragagt",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "oxlint": "^0.0.20",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
