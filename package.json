{
  "$schema": "https://json.schemastore.org/package",
  "name": "mktable",
  "keywords": [
    "markdown",
    "table",
    "contents",
    "cli"
  ],
  "description": "CLI tool that outputs a table of contents based on a markdown file headers",
  "version": "1.1.6",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbragagt/mktable.git"
  },
  "bin": {
    "mktable": "dist/index.mjs"
  },
  "scripts": {
    "postinstall": "npm run build",
    "test": "mocha -r tsx src/__tests__/*.ts",
    "build": "tsup src/index.ts --format esm",
    "lint": "oxlint src",
    "typecheck": "tsc --noEmit",
    "prepare": "[ -d '.husky' ] && husky install || true",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "README.md": [
      "./dist/index.js"
    ]
  },
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "chai": "^4.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "oxlint": "^0.0.20",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^4.5.4"
  },
  "author": "wesbragagt",
  "license": "MIT"
}
